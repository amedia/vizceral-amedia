{
  "name": "vizceral",
  "version": "0.0.4",
  "description": "Amedia fork of Vizceral",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "copy:fonts": "cpx \"./node_modules/source-sans-pro/**/*\" ./dist/fonts",
    "copy:json": "cpx \"./client/*.json\" ./dist",
    "dev-server": "webpack-dev-server --host=0.0.0.0 --port=9693",
    "dev": "npm-run-all clean --parallel copy:* dev-server",
    "build:client": "npm-run-all clean copy:*",
    "start": "NODE_ENV=production node bin/server.js | bunyan",
    "validate": "npm ls"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@amedia/amedia-access-log": "^1.2.1",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bootstrap": "^3.3.7",
    "bunyan": "^1.8.12",
    "compression": "^1.7.1",
    "convict": "^4.0.2",
    "cors": "^2.8.4",
    "css-loader": "^0.26.0",
    "eslint-loader": "^1.6.0",
    "express": "^4.16.2",
    "express-error-responses": "^0.3.0",
    "file-loader": "^0.9.0",
    "flux": "^3.1.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.0",
    "http": "0.0.0",
    "jquery": "^3.1.1",
    "keymirror": "^0.1.1",
    "keypress.js": "2.1.0-1",
    "lodash": "^4.16.4",
    "numeral": "^1.5.3",
    "query-string": "^4.2.3",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-virtualized": "^8.2.0",
    "socket.io-client": "^1.5.1",
    "source-sans-pro": "^2.0.10",
    "style-loader": "^0.13.1",
    "superagent": "^3.0.0",
    "tween.js": "^16.3.5",
    "url-loader": "^0.5.7",
    "vizceral-react": "~4.5.1",
    "webfontloader": "^1.6.26",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "cpx": "^1.5.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.4.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.1",
    "precommit-hook": "^3.0.0",
    "rimraf": "^2.5.4",
    "webpack-dev-server": "^1.16.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:amedia/vizceral-amedia.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
